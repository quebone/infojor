<?php
namespace Infojor\Presentation\Model;

class MainViewModel
{
	protected $model;
	protected $data;

	public function __construct($model = null)
	{
		$this->model = $model;
		$this->data = new \stdClass;
	}
	
	public function getModel()
	{
		return $this->model;
	}
	
	public function setModel($model)
	{
		$this->model = $model;
	}
}